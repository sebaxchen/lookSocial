{
  "name": "learning-center",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "build:dev": "ng build --configuration development",
    "build:prod": "ng build --configuration production",
    "build:gh-pages": "ng build --configuration githubPages && npm run copy-404",
    "copy-404": "node -e \"require('fs').copyFileSync('dist/learning-center/browser/index.html', 'dist/learning-center/browser/404.html')\"",
    "watch": "ng build --watch --configuration development",
    "preview": "ng serve --configuration production"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.2.4",
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/material": "^20.2.4",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "chart.js": "^4.5.1",
    "firebase": "^12.5.0",
    "lottie-web": "^5.13.0",
    "ng2-charts": "^8.0.0",
    "ngx-lottie": "^20.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "vercel": "^48.9.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.2",
    "@angular/cli": "^20.3.8",
    "@angular/compiler-cli": "^20.3.0",
    "typescript": "~5.9.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
